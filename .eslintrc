{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": { 
    "comma-dangle": [1, "always-multiline"],
    "no-console": "warn",
    "semi": "error",
    "@typescript-eslint/ban-ts-comment": "off"
  }
}